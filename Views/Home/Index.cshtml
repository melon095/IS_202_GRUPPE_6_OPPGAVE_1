@model string

@{
    ViewData["Title"] = "Home Page";
}

<section class="max-w-3xl mx-auto mt-10">
    <div class="text-center space-y-4">
        <h1 class="text-3xl sm:text-5xl font-bold tracking-tight text-indigo-700">
            Velkommen
        </h1>
        <a class="text-indigo-700 hover:underline"
           href="https://learn.microsoft.com/aspnet/core">
        </a>.

        <p></p>
    </div>

    @if (!string.IsNullOrWhiteSpace(Model))
    {
        <p class="mt-4 text-gray-800">@Model</p>
    }
   
    
    <p>Registrer ditt objekt med knappen under</p>
    <div class="mt-6">
        <a asp-action="DataForm" asp-controller="Obstacle"
           class="inline-flex items-center rounded-lg px-4 py-2 ring-1 ring-indigo-600 text-indigo-700 hover:bg-indigo-50">
            Registrer objekt
        </a>
    </div>
</section>